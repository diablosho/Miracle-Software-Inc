<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#000000"
	android:paddingBottom="@dimen/activity_vertical_margin"
	android:paddingLeft="@dimen/activity_horizontal_margin"
	android:paddingRight="@dimen/activity_horizontal_margin"
	android:paddingTop="@dimen/activity_vertical_margin"
	tools:context=".UI_Mainframe" >

	<TextView
		android:id="@+id/strX"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:text="@string/valueX"
		android:textColor="#FFFFFF" />

	<TextView
		android:id="@+id/strY"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignLeft="@+id/strX"
		android:layout_below="@+id/strX"
		android:layout_marginTop="23dp"
		android:text="@string/valueY"
		android:textColor="#FFFFFF" />

	<TextView
		android:id="@+id/strZ"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignLeft="@+id/strY"
		android:layout_below="@+id/strY"
		android:layout_marginTop="22dp"
		android:text="@string/valueZ"
		android:textColor="#FFFFFF" />

	<TextView
		android:id="@+id/strCos"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignLeft="@+id/strZ"
		android:layout_below="@+id/strZ"
		android:layout_marginTop="24dp"
		android:text="@string/valueCos"
		android:textColor="#FFFFFF" />

	<TextView
		android:id="@+id/strHeading"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignLeft="@+id/strCos"
		android:layout_below="@+id/strCos"
		android:layout_marginTop="20dp"
		android:text="@string/valueHeading"
		android:textColor="#FFFFFF" />

</RelativeLayout>